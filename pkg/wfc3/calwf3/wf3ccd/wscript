# vim: set syntax=python:

def build(bld):
    t = bld.program(
        source = 'mainccd.c',
        target = 'wf3ccd.e',
        use = ['calwf3', 'imphttab'] + bld.env.LOCAL_LIBS,
        lib = bld.env.EXTERNAL_LIBS,
        libpath = bld.env.LIBPATH,
        install_path = '${PREFIX}/bin'
        )

